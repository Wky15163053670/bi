<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
	<!-- namespace的地址为该pojo的mapper映射路径 -->
	<mapper namespace="com.bi.mapper.UserMapper">
		<!-- id为查询的唯一id，resultType为pojo实体名称 -->
	    <select id="list" resultType="User">
	        select * from user_t
	    </select>
	</mapper>

